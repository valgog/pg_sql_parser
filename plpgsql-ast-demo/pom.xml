  <dependencies>
  	<dependency>
  		<groupId>de.zalando.plgpqsql.ast</groupId>
  		<artifactId>plpgsql-ast</artifactId>
  		<version>1.0-SNAPSHOT</version>
  	</dependency>
    <dependency>
		<groupId>junit</groupId>
		<artifactId>junit</artifactId>
		<version>4.13.1</version>
		<scope>test</scope>
	</dependency>
